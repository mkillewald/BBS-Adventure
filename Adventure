  obj.rhindle.atk=0
  obj.rhindle.atkwait=delayedattack 
  obj.rhindle.lvl3.0=1   /* random room lower limit */
  obj.rhindle.lvl3.1=29  /* random room upper limit */
  obj.rhindle.dead=0
  
  fear.rhindle.max=0
  fear.rhindle.0="SWORD"
  
  want.rhindle.max=2
  want.rhindle.0="PL"
  want.rhindle.1="CHALICE"
  want.rhindle.2="WKEY"
return

