  room.cur=24; room.gate=-1; dark=0
  room.c="[0;47;31m"; room.fc="[0;31m"; room.bc="[47m"
  exit.t=26; exit.r=23; exit.b=26; exit.l=23; exit.ci=0; exit.co=0
